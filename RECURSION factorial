#include <bits/stdc++.h>
using namespace std;

//factorial de un numero por ejemplo  de n!=n*n-1*n-1 hasta que n=1

int factorial(int n){
    
   if(n<=0) n=1;
   else if(n>1) n= n*factorial(n-1); 
   
   return n; 
}



int main(){
    //recursiva con factoriales
    int n;
    cin>>n;
    cout<<factorial(n);
    
    
    return 0;
}
