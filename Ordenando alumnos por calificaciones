#include <bits/stdc++.h>
using namespace std;
int main(){
    vector<int>v ;
    
    int n,m;
    cin>>n;
    for(int i=0; i<n*2; i++){
        cin>>m;
        v.push_back(m);
        
        
    }
    
    sort(v.rbegin(),v.rend());
    for(int j=0; j<v.size(); j++){
        cout<<v[j]<<endl;
    }
    
    
    
    
    
    
 return 0;   
}
